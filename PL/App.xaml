<Application x:Class="PL.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:PL"
             StartupUri="LoginWindow.xaml">

    <Application.Resources>
        <local:CallCollection x:Key="CallCollection" />
        <local:JobsCollection x:Key="JobsCollectionKey" />
        <local:DistanceCollection x:Key="DistanceCollectionKey" />
        <local:VolunteerIdToHasLocationConverter x:Key="HasLocationConverter"/>
        <local:CallIdToHasLocationConverter x:Key="CallIdToHasLocationConverter"/>
        <local:ConvertUpdateToTrue x:Key="ConvertUpdateToTrueKey"/>
        <local:CallInProgressToStringConverter x:Key="CallInProgressConverter"/>
        <local:NullToVisibilityConverter x:Key="NullToVisibilityConverter"/>
        <local:NotNullToVisibilityConverter x:Key="NotNullToVisibilityConverter"/>
        <local:InverseBoolConverter x:Key="InverseBoolConverter"/>
        <local:BoolToVisibilityConverter x:Key="BoolToVisibilityConverter"/>
        <local:BoolToAddOrUpdateConverter x:Key="BoolToAddOrUpdateConverter"/>
        <local:SimulatorButtonTextConverter x:Key="SimulatorButtonTextConverter"/>

    </Application.Resources>
</Application>
